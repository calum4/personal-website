{
  "name": "personal-website",
  "version": "0.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/calum4/personal-website"
  },
  "license": "(MIT OR Apache-2.0)",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "npm run strip-config && ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "strip-config": "npm --prefix ./bin/strip-config-comments/ run start -- --input=../../config.json --output=./dist/config.stripped.json"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^20.1.0",
    "@angular/compiler": "^20.1.0",
    "@angular/core": "^20.1.0",
    "@angular/forms": "^20.1.0",
    "@angular/platform-browser": "^20.1.0",
    "@angular/router": "^20.1.0",
    "@ngrx/signals": "^20.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular/build": "^20.1.6",
    "@angular/cli": "^20.1.6",
    "@angular/compiler-cli": "^20.1.0",
    "@types/jasmine": "~5.1.0",
    "jasmine-core": "~5.8.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.8.2",
    "deep-utility-types": "^1.3.1"
  }
}
